&GLOBAL
  PROJECT testsuite_ad
  RUN_TYPE ENERGY_FORCE
  PREFERRED_DIAG_LIBRARY SL
  PRINT_LEVEL medium
&END GLOBAL
&MOTION
 &GEO_OPT
   MAX_DR 0.1E-8
   MAX_FORCE  0.1E-8
   MAX_ITER 900
   RMS_DR 0.1E-8
   RMS_FORCE 0.1E-8
 &END GEO_OPT
&END MOTION
&FORCE_EVAL
 &PRINT                   
  &FORCES
  &END FORCES
 &END PRINT 
 METHOD Quickstep
 &PROPERTIES
  &TDDFPT               
   &DIPOLE_MOMENTS
    DIPOLE_FORM VELOCITY
   &END DIPOLE_MOMENTS
   KERNEL FULL
   NSTATES 80
   MAX_ITER   100
   CONVERGENCE [eV] 1.0e-5
   RKS_TRIPLETS F
   &PRINT              
    &NAMD_PRINT
     PRINT_VIRTUALS T
     PRINT_PHASES T
    &END NAMD_PRINT
   &END PRINT
  &END TDDFPT
 &END PROPERTIES
  &DFT
    &QS
      METHOD GPW
     EPS_DEFAULT 1.0E-17
    &END QS
    &SCF
      SCF_GUESS restart
      &OT
         PRECONDITIONER FULL_ALL
         MINIMIZER DIIS
      &END OT
      &OUTER_SCF
        MAX_SCF 900
        EPS_SCF 1.0E-9
      &END OUTER_SCF
      MAX_SCF 50
      EPS_SCF 1.0E-9
    &END SCF
    POTENTIAL_FILE_NAME POTENTIAL
    BASIS_SET_FILE_NAME BASIS_MOLOPT
    BASIS_SET_FILE_NAME BASIS_ADMM_UZH
    &MGRID
      CUTOFF 800
      REL_CUTOFF 60
    &END MGRID
    &POISSON
       PERIODIC XYZ
       PSOLVER PERIODIC
    &END
    &XC
     &XC_FUNCTIONAL B3LYP
     &END XC_FUNCTIONAL
      &VDW_POTENTIAL
         POTENTIAL_TYPE PAIR_POTENTIAL
         &PAIR_POTENTIAL
            PARAMETER_FILE_NAME dftd3.dat
            TYPE DFTD3
            REFERENCE_FUNCTIONAL B3LYP
            R_CUTOFF [angstrom] 10
         &END
      &END VDW_POTENTIAL
      &HF
         FRACTION 0.2
         &HF_INFO OFF
         &END
         &INTERACTION_POTENTIAL
            POTENTIAL_TYPE TRUNCATED
            CUTOFF_RADIUS 1.9
            T_C_G_DATA t_c_g.dat
         &END
         &SCREENING
           EPS_SCHWARZ 1.0E-08
           EPS_SCHWARZ_FORCES 1.0E-06
           SCREEN_ON_INITIAL_P T
         &END
         &MEMORY
            MAX_MEMORY 3500
         &END
      &END HF
    &END XC
    &AUXILIARY_DENSITY_MATRIX_METHOD
      METHOD BASIS_PROJECTION
      ADMM_PURIFICATION_METHOD NONE
      EXCH_SCALING_MODEL NONE
      EXCH_CORRECTION_FUNC PBEX
    &END
  &END DFT
  &SUBSYS
    &CELL
       ABC 11.252000 3.831200 11.074000
       ALPHA_BETA_GAMMA 90.000000 117.150000  90.000000
      PERIODIC XYZ
    &END CELL
     &COORD
     @include coord.cp2k
     &END COORD
    &TOPOLOGY
     NATOMS 40    
     CONNECTIVITY OFF
    &END TOPOLOGY
    &KIND H
      BASIS_SET TZVP-MOLOPT-GTH
      BASIS_SET AUX_FIT admm-tzp-q1
      POTENTIAL GTH-PBE-q1
    &END KIND
    &KIND O
      BASIS_SET TZVP-MOLOPT-GTH
      BASIS_SET AUX_FIT admm-tzp-q6
      POTENTIAL GTH-PBE-q6
    &END KIND
    &KIND N
      BASIS_SET TZVP-MOLOPT-GTH
      BASIS_SET AUX_FIT admm-tzp-q5
      POTENTIAL GTH-PBE-q5
    &END KIND
    &KIND C
      BASIS_SET TZVP-MOLOPT-GTH
      BASIS_SET AUX_FIT admm-tzp-q4
      POTENTIAL GTH-PBE-q4
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
